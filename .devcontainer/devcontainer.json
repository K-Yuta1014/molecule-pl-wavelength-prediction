{
  "name": "mol-regression-env",
  "build": {
  "dockerfile": "Dockerfile",
  "context": "."
  }
,
  "settings": {
    "terminal.integrated.defaultProfile.linux": "bash"
  },
  "extensions": [
    "ms-python.python",
    "ms-toolsai.jupyter"
  ],
  "postCreateCommand": "python -m ipykernel install --user --name mol-regression-env --display-name 'Python (mol-regression-env)'",
  "runArgs": ["--gpus", "all"],
  "remoteUser": "root"
}
